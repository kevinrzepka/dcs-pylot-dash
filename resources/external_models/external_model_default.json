{
  "fields": {
    "mach": {
      "display_name": "Mach number",
      "lo_function": "LoGetMachNumber",
      "lo_return_type": "number",
      "unit": "none",
      "default_decimal_digits": 2
    },
    "airspeed": {
      "display_name": "Indicated Airspeed",
      "lo_function": "LoGetIndicatedAirSpeed",
      "lo_return_type": "number",
      "unit": "ms",
      "preferred_unit": "knots"
    },
    "tas": {
      "display_name": "True Airspeed",
      "lo_function": "LoGetTrueAirSpeed",
      "lo_return_type": "number",
      "unit": "ms",
      "preferred_unit": "knots"
    },
    "vertical_speed": {
      "display_name": "Vertical Speed",
      "lo_function": "LoGetVerticalVelocity",
      "lo_return_type": "number",
      "unit": "ms",
      "preferred_unit": "fts"
    },
    "pilot_name": {
      "display_name": "Pilot Name",
      "lo_function": "LoGetPilotName",
      "lo_return_type": "string"
    },
    "altitude_msl": {
      "display_name": "Altitude (MSL)",
      "lo_function": "LoGetAltitudeAboveSeaLevel",
      "lo_return_type": "number",
      "unit": "meters",
      "preferred_unit": "feet"
    },
    "altitude_agl": {
      "display_name": "Altitude (AGL)",
      "lo_function": "LoGetAltitudeAboveGroundLevel",
      "lo_return_type": "number",
      "unit": "meters",
      "preferred_unit": "feet"
    },
    "mag_heading": {
      "display_name": "Mag. Heading",
      "lo_function": "LoGetMagneticYaw",
      "lo_return_type": "number",
      "unit": "radians",
      "preferred_unit": "degrees"
    },
    "self": {
      "lo_function": "LoGetSelfData",
      "lo_return_type": "table",
      "fields": {
        "Pitch": {
          "display_name": "Pitch Angle",
          "lo_return_type": "number",
          "unit": "radians",
          "preferred_unit": "degrees"
        },
        "Bank": {
          "display_name": "Bank Angle",
          "lo_return_type": "number",
          "unit": "radians",
          "preferred_unit": "degrees"
        },
        "Heading": {
          "display_name": "Heading",
          "lo_return_type": "number",
          "unit": "radians",
          "preferred_unit": "degrees"
        },
        "LatLongAlt": {
          "lo_return_type": "table",
          "fields": {
            "Lat": {
              "display_name": "Latitude",
              "lo_return_type": "number",
              "unit": "degrees",
              "default_decimal_digits": 5
            },
            "Long": {
              "display_name": "Longitude",
              "lo_return_type": "number",
              "unit": "degrees",
              "default_decimal_digits": 5
            }
          }
        }
      }
    },
    "engine_info": {
      "lo_function": "LoGetEngineInfo",
      "lo_return_type": "table",
      "fields": {
        "fuel_internal": {
          "display_name": "Fuel (total)",
          "lo_return_type": "number",
          "unit": "pounds",
          "abs_base_value": 12000,
          "is_portion": true
        }
      }
    },
    "payload_info": {
      "lo_function": "LoGetPayloadInfo",
      "lo_return_type": "table",
      "fields": {
        "Cannon": {
          "lo_return_type": "table",
          "fields": {
            "shells": {
              "display_name": "Gun Rounds",
              "lo_return_type": "number"
            }
          }
        }
      }
    }
  }
}
